grades = [[5, 3, 3, 5, 4], [2, 2, 2, 3], [4, 5, 5, 2], [4, 4, 3], [5, 5, 5, 4, 5]]
students = {'Johnny', 'Bilbo', 'Steve', 'Khendrik', 'Aaron'}
import statistics
grades_sr = [statistics.fmean(grades[0]), statistics.fmean(grades[1]), statistics.fmean(grades[2]),statistics.fmean(grades[3]),statistics.fmean(grades[4])]
students_sp = list (students)
students_sp.sort()
SR_Ball = dict (zip (students_sp,grades_sr))
print (SR_Ball)
